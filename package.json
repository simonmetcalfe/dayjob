{
  "name": "dayjob",
  "version": "0.0.8",
  "description": "Make great Spotify playlists while you work.",
  "author": "Simon Metcalfe",
  "keywords": [
    "Spotify",
    "playlist",
    "dj",
    "music",
    "keyboard shortcuts"
  ],
  "license": "TBC",
  "main": "src/main.js",
  "dependencies": {
    "bootstrap": "4.1.3",
    "electron-log": "3.0.1",
    "electron-packager": "14.0.6",
    "electron-store": "3.0.0",
    "font-awesome": "^4.6.1",
    "jquery": "3.3.1",
    "menubar": "5.2.3",
    "npm": "6.12.0",
    "popper.js": "1.14.7",
    "request": "2.87.0",
    "spotify-web-api-node": "4.0.0",
    "tether": "1.4.5"
  },
  "devDependencies": {
    "electron": "4.1.1",
    "electron-installer-dmg": "latest",
    "electron-installer-windows": "latest",
    "electron-rebuild": "1.8.6"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --platform=darwin --arch=x64 --overwrite --out=build-temp",
    "package-mac": "electron-installer-dmg ./build-temp/dayjob-darwin-x64/dayjob.app ./installers/osx/dayjob",
    "package-win": "electron-installer-windows --src build-temp/dayjob-win32-x64/ --dest installers/windows/dayjob"
  },
  "repository": ""
}
